				MOV		R0, #46  ;DIVIDENDO
				MOV		R1, #9	;DIVISOR
				MOV		R5, #0X100 ;DIRECCION DONDE GUARDA EL COCIENTE
				MOV		R6, #0X200 ;DIRECCION DONDE GUARDA EL RESIDUO
				MOV		R2, #0     ;COCIENTE
				MOV		R3, #0     ;RESIDUO
				MOV		R4, #0     ;VARIABLE USADA PARA CONTROLAR LA MULTIPLICACIÓN
Multiplicar
				
				CMP		R0,R4
				BEQ		TERMINAR ; SI SON IGUALES SE PROCEDE NORMALMENTE
				BLE		RESIDUO_COCIENTE ; SI LA VARIABLE QUE CONTROLA ES MAYOR AL DIGITO ORIGINAL, SE DEBE REDUCIR A LA CANTIDAD PROXIMA MÁS CERCANA
				ADD		R4,R4,R1
				ADD		R2,R2,#1
				b		Multiplicar
				
				
RESIDUO_COCIENTE ;elimina el exceso
				SUB		R2,R2,#1
				SUB		R4,R4,R1
				SUB		R3,R0,R4
TERMINAR
